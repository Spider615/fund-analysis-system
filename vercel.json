{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "functions": {
    "api/funds.js": {
      "maxDuration": 30
    },
    "api/analyze.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/funds",
      "dest": "/api/funds.js"
    },
    {
      "src": "/api/analyze",
      "dest": "/api/analyze.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}